{
  "name": "capstone-stash",
  "private": "false",
  "version": "1.0.0",
  "description": "Ada C6 Capstone Bot Project",
  "scripts": {
    "create": "claudia create --region us-east-1 --api-module bot",
    "configure-facebook": "claudia update --configure-fb-bot",
    "configure-skype": "claudia update --configure-skype-bot",
    "configure-telegram": "claudia update --configure-telegram-bot",
    "configure-slack": "claudia update --timeout 120 --allow-recursion --configure-slack-slash-command",
    "start": "ntl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nahmisa/capstone-stash.git"
  },
  "license": "MIT",
  "dependencies": {
    "claudia-bot-builder": "^1.0.0",
    "aws-sdk": "^2.7.20",
    "promise-delay": "^2.1.0"
  },
  "devDependencies": {
    "claudia": "^1.9.0",
    "ntl": "^1.1.3"

  }
}
